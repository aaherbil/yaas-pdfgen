{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title":"Template",
  "type":"object",
  "properties":
  {
    "id":
    {
      "type":"string"
    },
    "title":
    {
      "type":"string",
      "pattern":"^.+"
    },
    "description":
    {
      "type":"string",
      "pattern":"^.+"
    },
    "file":
    {
      "type":"string"
    },
    "createdAt":
    {
      "type":"string",
      "format":"date-time"
    }
  },
  "required":["title"]
}
